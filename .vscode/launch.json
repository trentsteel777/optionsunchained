{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "fetch_options", // The name displayed in the debug dropdown menu
            "type": "cppdbg",       // For C/C++ debugging
            "request": "launch",
            "program": "${workspaceFolder}/yahoo_scraper/fetch_options", // Path to your executable
            "args": [],             // Command-line arguments for your program (optional)
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/yahoo_scraper",
            "environment": [],
            "externalConsole": false, // Set true if you want an external terminal
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb", // Specify the path to your debugger here
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}